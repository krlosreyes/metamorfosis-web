---
// This component is not used directly, but its existence ensures Tailwind generates these classes
---

<div class="hidden">
    <!-- Pro Tip Box -->
    <div
        class="bg-emerald-50 border-l-4 border-emerald-500 p-6 my-8 rounded-r-xl shadow-sm flex items-center gap-3 mb-2 text-2xl font-bold text-emerald-800 m-0 text-emerald-900"
    >
    </div>

    <!-- Scientific Fact Box -->
    <div
        class="bg-blue-50 border-l-4 border-blue-500 text-blue-800 text-blue-900 uppercase tracking-wide leading-relaxed font-medium"
    >
    </div>

    <!-- Process Flow -->
    <div
        class="flex flex-col md:flex-row justify-center gap-4 bg-gray-50 rounded-2xl border-gray-100 shadow-inner text-center font-bold text-lg text-gray-700 text-gray-300 transform rotate-90 md:rotate-0 text-red-600 text-gray-500"
    >
    </div>

    <!-- Features / Benefits List -->
    <div
        class="grid grid-cols-1 md:grid-cols-2 bg-white border-emerald-100 p-4 rounded-xl text-xl font-semibold"
    >
    </div>

    <!-- Checkmarks -->
    <div
        class="bg-green-100 text-green-700 rounded-full w-6 h-6 flex justify-center items-center text-xs"
    >
    </div>

    <!-- Warning Box -->
    <div class="bg-gray-900 text-white shadow-xl mb-2 text-gray-300"></div>

    <!-- Comparison List -->
    <div
        class="space-y-4 justify-between bg-red-50 border-red-100 text-red-900 text-red-600 bg-green-50 border-green-100 text-green-900 text-green-600"
    >
    </div>

    <!-- Gradient Box -->
    <div
        class="bg-gradient-to-r from-emerald-500 to-teal-600 rounded-2xl p-8 mb-6 list-decimal list-inside text-emerald-50"
    >
    </div>

    <!-- Utilities -->
    <div class="space-y-3 my-6 list-none pl-0 mt-1.5 space-y-2 mt-2"></div>
</div>
